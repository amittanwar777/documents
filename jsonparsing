#!/bin/bash

# Load JSON file
JSON=$(cat A.json)

# Parse JSON using 'jq' command and write to a temporary file
echo "$JSON" | jq -r 'to_entries[] | "\(.key)=\(.value)"' > temp.txt

# Read the temporary file and export as environment variables
while IFS='=' read -r key value; do
  echo "$key"="$value"
done < temp.txt

# Remove the temporary file
rm temp.txt

# For validation, you can print the environment variables
